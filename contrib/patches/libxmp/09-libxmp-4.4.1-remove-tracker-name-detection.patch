-			} else {
-				snprintf(tracker_name, 40,
-					 "Schism Tracker 0.%x", cwtv);
-			}
-			break; }
-		case 0x5:
-			snprintf(tracker_name, 40, "OpenMPT %d.%02x",
-				 (ifh->cwt & 0x0f00) >> 8, ifh->cwt & 0xff);
-			if (memcmp(&ifh->rsvd, "OMPT", 4))
-				strncat(tracker_name, " (compat.)", 39);
-			break;
-		case 0x06:
